<h1>Create an account!</h1>
<%= form_with model: User.new, local: true do |f| %>
  <%= f.label "Email: " %>
  <%= f.email_field :email %><br>
  <%= f.label "Password: " %>
  <%= f.password_field :password %><br>
  <%= f.label "Confirm: " %>
  <%= f.password_field :password_confirmation %><br>
  <%= f.submit "Create Account" %>
<% end %>
